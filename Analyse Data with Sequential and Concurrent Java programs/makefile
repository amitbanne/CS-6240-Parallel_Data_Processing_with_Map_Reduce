default: run
# makefile should be present in the same directory as pom file, and src directory
# jar is generated by maven (inside target directory)
run:
	mvn clean install
# execute the generated jar by passing input file path
# (current definition needs input file should be inside the same directory as makefile)
local:
	java -jar target/WeatherData-0.0.1-SNAPSHOT-jar-with-dependencies.jar 1912.csv
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
